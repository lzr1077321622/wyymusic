{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
     "transform-runtime",
     ["import", { //按需引入配置vant
      "libraryName": "vant",
      "libraryDirectory": "es",
      "style": true
    }]
    ]
}
